{
  "name": "morado-frontend",
  "private": true,
  "version": "0.1.0",
  "description": "Morado Testing Platform - Vue3 Frontend Application",
  "type": "module",
  "author": "mcgeq",
  "license": "MIT",
  "keywords": [
    "testing",
    "automation",
    "vue3",
    "test-platform"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "format": "bun biome format --write .",
    "format:check": "bun biome format .",
    "lint": "bun biome lint .",
    "lint:fix": "bun biome lint --write .",
    "check": "bun biome check --write .",
    "ci": "bun biome ci .",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@tailwindcss/vite": "^4.1.18",
    "axios": "^1.13.2",
    "echarts": "^6.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-echarts": "^8.0.1",
    "vue-i18n": "9",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.16",
    "@vue/tsconfig": "^0.8.1",
    "fast-check": "^4.5.2",
    "jsdom": "^27.3.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^8.0.0-beta.3",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.4"
  }
}
